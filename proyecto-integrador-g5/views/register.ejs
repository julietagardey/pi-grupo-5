<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
    <%- include("partials/head.ejs") %>


<body>
	<!-- Header -->
    <%- include("partials/header-logoff.ejs") %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Registrate</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> 
				<br> 
				<form action="/users/profile" method="get">
					<div>
						<label>Email:</label>
				<input id="email" type="email" name="email" value="">
				</div>
					<div>
						<label>Usuario:</label>
				<input id="usuario" type="text" name="Usuario" value="">
				</div>
					<div>
						<label>Contraseña:</label>
				<input id="contraseña" type="password" name="contraseña" value="">
				</div>
					<div>
						<label>Fecha de nacimiento:</label>
				<input id="fechaDeNacimiento" type="date" name="fechaDeNacimiento" value="">
				</div>
					<div>
						<label>Número de documento:</label>
				<input id="numeroDeDocumento" type="number" name="numeroDeDocumento" value="">
				</div>
					<div>
						<label>Foto de Perfil:</label>
				<input id="fotoPerfil" type="text" name="fotoPerfil" value="">
				</div>
					<div>
						<button type="submit">Registrar</button> 
				</div>
				</form>
			<br>
			</h2>
		</div>
	</div>
	<!-- footer -->
    <%- include("partials/footer.ejs") %>
	<!-- /footer -->
</body>

</html>